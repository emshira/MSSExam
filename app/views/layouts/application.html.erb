  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed" rel="stylesheet">
</head>
<body>

  <body>

    <header class="nav">
      <nav class="navbar navbar-static-top" style="background-color: #42253B;">
        <div class="nav-active">
          <% if current_practitioner  %>
          Signed in as <%= current_practitioner.email %> | <%= link_to "Logout", '/logout' %>
          <% else %>
          <%= link_to 'Login', '/login' %> | <%= link_to 'Create Account', new_practitioner_path %>
          <% end %>
        </div>
        <div class="home-link">
          <p>
            <%= link_to 'Home', practitioners_path %>
          </p>
        </div>
      </nav>
    </header>

    <%= yield %>

  </body>
</head>
</html>
