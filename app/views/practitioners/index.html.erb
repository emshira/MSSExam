<h1>MiniMed Exam</h1>

<h3>Practitioner Log in</h3>
<%= form_tag practitioners_path do %>
<p>
  <%= label_tag :identification %>
  <%= text_field_tag :identification %>
</p>
<p>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</p>
<p>
  <%= submit_tag "Log in" %>
</p>
<% end %>

<%= link_to 'New Practitioner', new_practitioner_path %>
